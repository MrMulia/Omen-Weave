 }); // New state for threat hunting leads\n  const [threatIntelligence, setThreatIntelligence] = useState([]);\n  const [alertColor, setAlertColor] = useState('#606060');\n  const [isSidebarOpen, setIsSidebarOpen] = useState(false); // State for sidebar visibility\n\n  useEffect(() => {\n    const ws = new WebSocket('ws://localhost:5001/ws');\n\n    ws.onopen = () => {\n      console.log('WebSocket connection opened');\n    };\n\n    ws.onmessage = (event) => {\n      const data = JSON.parse(event.data);\n      console.log('Received data:', data);\n\n      setAlertColor(getAlertColor(data.network_alert));\n      updateRealtimeChart(data.entropy || 0); // Add default value for entropy\n      setTopNotableSources(data.top_notable_sources || []); // Add default value for top_notable_sources\n      setSeverityData(data.detection_severity || [0, 0, 0, 0]); // Add default value for detection_severity\n      updateScoreChart(data.current_score || 0); // Add default value for current_score\n      setRecentDetections(data.most_recent_detections || []); // Add default value for most_recent_detections\n      setShaDetections(data.sha_detections || []); // Set SHA-based detections\n      setThreatHuntingLeads({ \n        investigated: data.threat_hunting_leads_investigated || 0, \n        generated: data.threat_hunting_leads_generated || 0 \n      }); // Set Threat Hunting Leads\n      setThreatIntelligence(data.threat_intelligence || []);\n    };\n\n    ws.onclose = () => {\n      console.log('WebSocket connection closed');\n    };\n\n    ws.onerror = (error) => {\n      console.log('WebSocket error:', error);\n    };\n\n    return () => {\n      ws.close(); // Clean up WebSocket connection on unmount\n    };\n  }, []);\n\n  const getAlertColor = (level) => {\n    switch (level) {\n      case 'Fetching Data': return '#606060';\n      case 'Low': return '#008000';\n      case 'Medium': return '#FFA500';\n      case 'High': return '#FF4500';\n      case 'HIGH ALERT!': return '#B22222';\n      default: return '#606060';\n    }\n  };\n\n  const updateRealtimeChart = (value) => {\n    setEntropyData(prevData => {\n      const newData = [...prevData, value];\n      return newData.length > 7 ? newData.slice(1) : newData; // Keep last 8 data points (40 seconds of data at 5-second intervals)\n    });\n    setEntropyLabels(prevLabels => {\n      const newLabels = [...prevLabels, new Date().toLocaleTimeString()];\n      return newLabels.length > 8? newLabels.slice(1) : newLabels;\n    });\n  };\n\n  const updateScoreChart = (score) => {\n    setScoreData(prevData => {\n      const newData = [...prevData, score];\n      return newData.length > 7 ? newData.slice(1) : newData;\n    });\n    setScoreLabels((prevLabels) => {\n      const newLabels = [...prevLabels, new Date().toLocaleTimeString()];\n      return newLabels.length > 8? newLabels.slice(1) : newLabels;\n    });\n  };\n\n  const scoreChartData = {\n    labels: scoreLabels,\n    datasets: [{\n      label: 'Current Score',\n      data: scoreData,\n      backgroundColor: 'rgba(255, 99, 132, 0.2)',\n      borderColor: 'rgba(255, 99, 132, 1)',\n      borderWidth: 2,\n      pointRadius: 0, // Remove dots\n      fill: false\n    }]\n  };\n\n  const severityChartData = {\n    labels: ['Low', 'Medium', 'High', 'Critical'],\n    datasets: [{\n      label: 'Detections by Severity',\n      data: severityData,\n      backgroundColor: [\n        'rgba(0, 0, 255, 0.75)', // Bolder blue\n        'rgba(255, 165, 0, 0.75)', // Bolder orange\n        'rgba(255, 69, 0, 0.75)', // Bolder orange red\n        'rgba(255, 0, 0, 0.75)' // Bolder red\n      ],\n      borderColor: [\n        'rgba(0, 0, 255, 0.5)', // Bolder blue\n        'rgba(255, 165, 0, 0.5)', // Bolder orange\n        'rgba(255, 69, 0, 0.5)', // Bolder orange red\n        'rgba(255, 0, 0, 0.5)' // Bolder red\n      ],\n      borderWidth: 1\n    }]\n  };\n\n  const entropyChartData = {\n    labels: entropyLabels,\n    datasets: [{\n      label: 'Entropy',\n      data: entropyData,\n      backgroundColor: 'rgba(54, 162, 235, 0.5)',\n      borderColor: 'rgba(0, 0, 139, 1)', // Darker blue\n      borderWidth: 2,\n      pointRadius: 0, // Remove dots\n      fill: false\n    }]\n  };\n\n  const chartOptions = {\n    responsive: true,\n    maintainAspectRatio: false,\n    plugins: {\n      legend: {\n        display: false // Hide legend\n      }\n    },\n    scales: {\n      x: {\n        grid: {\n          display: false // Hide X grid lines\n        }\n      },\n      y: {\n        grid: {\n          display: false // Hide Y grid lines\n        }\n      }\n    }\n  };\n\n  const entropyChartOptions = {\n    ...chartOptions,\n    scales: {\n      ...chartOptions.scales,\n      y: {\n        ...chartOptions.scales.y,\n        min: -1,\n        max: 1\n      }\n    },\n    layout: {\n      padding: {\n        top: 20,\n        bottom: 35 // Adjust padding to reduce the height of the chart\n      }\n    }\n  };\n\n  return (\n    <div>\n      <header>\n        <div className=\"logo\">\n          <a href=\"#\" onClick={() => setIsSidebarOpen(true)}>Omen & Weave</a>\n        </div>\n        <nav>\n          <ul>\n            <li><a href=\"#\">Platform</a></li>\n            <li><a href=\"#\">Services</a></li>\n            <li><a href=\"#\">Why Omen & Weave</a></li>\n            <li><a href=\"#\">Learn</a></li>\n            <li><a href=\"#\">Company</a></li>\n          </ul>\n        </nav>\n        <div className=\"header-buttons\">\n          <button>Contact Us</button>\n          <button>View Bundles & Pricing</button>\n        </div>\n      </header>\n      <Sidebar isOpen={isSidebarOpen} onClose={() => setIsSidebarOpen(false)} />\n      <div className=\"dashboard-header\">\n        <h1>Detections Dashboard</h1>\n        <div id=\"networkStatus\" className=\"circle\" style={{ backgroundColor: alertColor }}></div>\n      </div>\n      <div className=\"dashboard\">\n        <div className=\"module\" id=\"currentScore\">\n          <h2>Current OWL Score</h2>\n          <div className=\"chart-container\">\n            <Line data={scoreChartData} options={chartOptions} />\n          </div>\n        </div>\n        <div className=\"module\" id=\"topNotableSources\">\n          <h2>Top Notable Sources</h2>\n          <ul>\n            {topNotableSources.map((source, index) => (\n              <li key={index}>{source}</li>\n            ))}\n          </ul>\n        </div>\n        <div className=\"module\" id=\"shaDetections\">\n          <h2>SHA-based Detections</h2>\n          <ul>\n            {shaDetections.map((detection, index) => (\n              <li key={index}>{detection}</li>\n            ))}\n          </ul>\n        </div>\n        <div className=\"module\" id=\"threatHuntingLeads\">\n          <h2>Threat Hunting Leads</h2>\n          <div>\n            Investigated / Generated: {threatHuntingLeads.investigated} / {threatHuntingLeads.generated}\n          </div>\n        </div>\n        <div className=\"module chart-container\" id=\"entropyOverTime\">\n          <h2>Entropy Over Time</h2>\n          <Line data={entropyChartData} options={entropyChartOptions} />\n        </div>\n        <div className=\"module\" id=\"detectionSeverity\">\n          <h2>Detections by Severity</h2>\n          <div className=\"chart-container\">\n            <Bar data={severityChartData} options={chartOptions} />\n          </div>\n        </div>\n        <div className=\"module\" id=\"threatIntelligence\">\n          <h2>Threat Intelligence</h2>\n          <ul>\n            {threatIntelligence.map((intelligence, index) => (\n              <li key={index}>{intelligence}</li>\n            ))}\n          </ul>\n        </div>\n        <div className=\"module\" id=\"recentDetections\">\n          <h2>Most Recent Detections</h2>\n          <ul>\n            {recentDetections.map((detection, index) => (\n              <li key={index}>{detection}</li>\n            ))}\n          </ul>\n        </di